{
  "name": "leanne1-webpack4-hmr-express",
  "version": "0.0.0",
  "description": "Boilerplate for a React application built using Webpack 4 with HMR on a custom Express server",
  "main": "server/index.js",
  "scripts": {
    "dev": "NODE_ENV=development babel-node src/server/index.js",
    "dev:server": "NODE_ENV=development nodemon src/server/index.js --exec babel-node",
    "prestart": "NODE_ENV=production webpack && babel src/server -d dist/server",
    "start": "NODE_ENV=production node dist/server/index.js"
  },
  "author": "leanne1",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "express": "^4.15.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "connect-history-api-fallback": "^1.5.0",
    "css-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^7.0.0",
    "nodemon": "^1.17.5",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.3.3",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^3.0.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  }
}
